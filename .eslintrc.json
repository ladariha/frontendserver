{
    "env": {
        "browser": true,
        "es6": true,
        "node" : true
    },
    "globals": {
        "$": true,
        "ko": true,
        "define": true,
        "require": true,
        "requirejs": true,
        "CORE": true,
        "Logger": true,
        "expect" : true,
        "it" : true,
        "beforeEach" : true,
        "afterEach" : true,
        "describe" : true,
        "oj": true
    },
    "rules": {
        "comma-dangle": [2, "never"],
        "no-cond-assign": [2, "always"],
        "no-constant-condition": 1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-semi": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-regex-spaces": 2,
        "no-obj-calls": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "valid-typeof": 2,
        "strict": [2, "global"],
        "default-case": 2,
        "curly": 2,
        "eqeqeq": 2,
        "no-alert": 2,
        "no-console": 2,
        "no-case-declarations": 2,
        "no-labels": 2,
        "no-unused-labels": 2,
        "no-unused-expressions": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-implied-eval": 2,
        "no-loop-func": 2,
        "no-lone-blocks": 2,
        "no-native-reassign": 2,
        "no-new-wrappers": 2,
        "no-return-assign": 2,
        "no-with": 2,
        "camelcase": 2,
        "quotes": [2, "double"],
        "semi": [2, "always"],
        "no-unused-vars": 2,
        "no-undefined": 2,
        "no-undef-init": 2,
        "no-undef": 2,
        "no-use-before-define": 2
    }
}
